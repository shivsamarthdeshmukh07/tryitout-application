package com.cdac;

public class BinarySearchInArray {
	
	public static int checkBianrySearch(int arr[],int key) {
		
		int l = 0;
		int r = arr.length - 1;
		
		
		while(l<r) {
			int m = l +((r-l)/2);
			if(arr[m]==key) {
				return m;
			}
			else if(arr[m]<key) {
				r = m;
			}
			else {
				l = m;
			}
		}
		return -1;
		
	}
}
